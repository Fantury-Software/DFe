<?xml version="1.0" encoding="utf-8"?>
<Configuracoes>
	<Servicos ID="NFSe">
		<GerarNfse versao="1.00">
			<Descricao>Envio de lote RPS - Sincrono</Descricao>
			<RequestURIProducao>https://sefin.producaorestrita.nfse.gov.br/sefinnacional/nfse</RequestURIProducao>
			<RequestURIHomologacao>https://sefin.producaorestrita.nfse.gov.br/sefinnacional/nfse</RequestURIHomologacao>
			<MetodoAPI>post</MetodoAPI>
			<WebActionProducao>dpsXmlGZipB64</WebActionProducao>
			<TagAssinatura>DPS</TagAssinatura>
			<TagAtributoID>infDPS</TagAtributoID>
			<WebSoapString>{xml}</WebSoapString>
			<WebContentType>application/json</WebContentType>
			<WebTagRetorno>nfseXmlGZipB64</WebTagRetorno>
			<SchemaVersao></SchemaVersao>
			<GZIPCompress>true</GZIPCompress>
		</GerarNfse>
		<CancelarNfse versao="1.00">
			<Descricao>Envio do cancelamento de NFSe</Descricao>
