			<TagAssinatura></TagAssinatura>
			<TagAtributoID></TagAtributoID>
			<WebSoapString>{xml}</WebSoapString>
			<WebContentType>application/json</WebContentType>
			<WebTagRetorno>nfseXmlGZipB64</WebTagRetorno>
			<SchemaVersao></SchemaVersao>
			<GZIPCompress>false</GZIPCompress>
