			<WebContentType>application/json</WebContentType>
			<WebTagRetorno>eventoXmlGZipB64</WebTagRetorno>
			<SchemaVersao></SchemaVersao>
			<GZIPCompress>true</GZIPCompress>
		</CancelarNfse>
		<ConsultarNfsePorRps versao="1.00">
			<Descricao>Consultar NFSe</Descricao>
			<RequestURIProducao>https://sefin.producaorestrita.nfse.gov.br/sefinnacional/nfse/{ChaveAcesso}</RequestURIProducao>
			<RequestURIHomologacao>https://sefin.producaorestrita.nfse.gov.br/sefinnacional/nfse/{ChaveAcesso}</RequestURIHomologacao>
			<MetodoAPI>get</MetodoAPI>
