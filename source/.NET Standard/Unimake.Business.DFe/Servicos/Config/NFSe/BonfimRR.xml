			<RequestURIHomologacao>https://sefin.producaorestrita.nfse.gov.br/sefinnacional/nfse/{ChaveAcesso}/eventos</RequestURIHomologacao>
			<MetodoAPI>post</MetodoAPI>
			<WebActionProducao>pedidoRegistroEventoXmlGZipB64</WebActionProducao>
			<TagAssinatura></TagAssinatura>
			<TagAtributoID></TagAtributoID>
